{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/admin",
      "dest": "/admin.html"
    },
    {
      "src": "/admin.html",
      "dest": "/admin.html"
    },
    {
      "src": "/print-report",
      "dest": "/print-report.html"
    },
    {
      "src": "/print-report.html",
      "dest": "/print-report.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
